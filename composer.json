{
  "name": "nettrine/dbal",
  "description": "Doctrine DBAL for Nette Framework",
  "keywords": [
    "nette",
    "doctrine",
    "dbal",
    "database",
    "mysql",
    "postgres",
    "sqlite"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-dbal",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "doctrine/dbal": "^4.2.1",
    "nette/di": "^3.2.2",
    "psr/log": "^3.0",
    "psr/cache": "^3.0.0"
  },
  "require-dev": {
    "ext-pdo": "*",
    "contributte/qa": "^0.4.0",
    "contributte/tester": "^0.3.0",
    "contributte/phpstan": "^0.2.0",
    "contributte/console": "^0.10.1",
    "mockery/mockery": "^1.3.5",
    "symfony/cache": "^7.1.9",
    "monolog/monolog": "^3.8.0",
    "tracy/tracy": "^2.10.3"
  },
  "conflict": {
    "nette/di": "<3.0.6",
    "nette/schema": "<1.1.0",
    "doctrine/event-manager": "<2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\DBAL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.10.x-dev"
    }
  }
}
